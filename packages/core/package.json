{
  "name": "@better-docusign/core",
  "version": "0.4.0",
  "description": "Core types and thin-path helpers for Docusign",
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/owner/better-docusign-node#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/owner/better-docusign-node.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/owner/better-docusign-node/issues"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "typecheck": "tsc -p . --noEmit",
    "prepack": "pnpm run build",
    "gen:esign": "openapi-typescript https://raw.githubusercontent.com/docusign/OpenAPI-Specifications/master/esignature.rest.swagger-v2.1.json -o src/generated/esignature.d.ts"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "sourcemap": true,
    "clean": true,
    "target": "es2022",
    "outDir": "dist",
    "platform": "neutral",
    "treeshake": true,
    "minify": false
  },
  "devDependencies": {
    "openapi-typescript": "^7.4.2",
    "tsup": "^8.0.1",
    "typescript": "^5.5.4"
  }
}
