{
  "name": "@better-docusign/core",
  "version": "0.1.0",
  "description": "Core types and thin-path helpers for DocuSign",
  "type": "module",
  "files": ["dist"],
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepack": "pnpm run build",
    "gen:esign": "openapi-typescript https://raw.githubusercontent.com/docusign/OpenAPI-Specifications/master/esignature.rest.swagger-v2.1.json -o src/generated/esignature.d.ts"
  },
  "tsup": {
    "entry": ["src/index.ts"],
    "format": ["esm", "cjs"],
    "dts": true,
    "sourcemap": true,
    "clean": true,
    "treeshake": true,
    "minify": false,
    "target": "es2022",
    "outDir": "dist",
    "platform": "neutral"
  },
  "devDependencies": {
    "openapi-typescript": "^7.4.2",
    "tsup": "^8.0.1",
    "typescript": "^5.5.4"
  }
}
