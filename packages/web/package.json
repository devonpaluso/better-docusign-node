{
  "name": "@better-docusign/web",
  "version": "0.2.0",
  "description": "Browser helpers to call your backend and open embedded signing",
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "typecheck": "tsc -p . --noEmit",
    "prepack": "pnpm run build"
  },
  "dependencies": {
    "@better-docusign/core": "workspace:^"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "sourcemap": true,
    "clean": true,
    "target": "es2022",
    "outDir": "dist",
    "platform": "browser",
    "external": [
      "@better-docusign/core"
    ],
    "treeshake": true,
    "minify": false
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.5.4"
  }
}
